<odoo>
    <data>
        <!--        Sequence for hospital.patient   -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_branch_allocation" model="ir.sequence">
            <field name="name">Reference</field>
            <field name="code">branch.allocation.line</field>
            <field name="prefix">REF</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>